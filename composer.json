{
  "name": "zanphp/nosql",
  "require": {
    "php": ">=5.6",
    "zanphp/contracts": "dev-master",
    "zanphp/container": "dev-master",
    "zanphp/coroutine": "dev-master",
    "zanphp/exception": "dev-master",
    "zanphp/support": "dev-master"
  },
  "require-dev": {
    "zanphp/connection-pool": "dev-master",
    "zanphp/spi":"dev-master",
    "zanphp/config":"dev-master",
    "zanphp/testing": "dev-master"
  },
  "autoload": {
    "psr-4": {
      "ZanPHP\\NoSql\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ZanPHP\\NoSql\\Tests\\": "tests"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "config": {
    "preferred-install": "dist",
    "secure-http": false
  },
  "minimum-stability": "dev"
}